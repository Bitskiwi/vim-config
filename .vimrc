syntax on
set mouse=                               "no mouse no problem
set number
set relativenumber
set laststatus=2
set autoindent
set tabstop=4
set shiftwidth=4
set whichwrap+=<,>,h,l
set backspace=indent,eol,start
set fillchars+=eob:\|
set list
set listchars=tab:\|-,trail:-
set cursorline
set wildmenu
colorscheme retrobox 
